<!DOCTYPE html>
<html lang="en">
<head>
    <title>Searched</title>
    <%- include('../layouts/head') %> 

</head>
<body>
    <header>
        <%- include('../layouts/header') %> 


    </header>

    <main>
        <div class="wrap">
            <% data.forEach(e=>{ %>

                <div class="container">
                    <h3>
                        <%= e.country %> 
                    </h3>
                    <h3>
                        Number Of Cases: <%= e.cases %> 
                    </h3>
                    <h3>
                        Date: <%= e.date %> 
                    </h3>

                </div>

            <%}) %> 
        </div>
    </main>
    
</body>
</html>